.vertical-alignment-helper {
    display:table;
    height: 100%;
    width: 100%;
    pointer-events:none;
}
.vertical-align-center {
    /* To center vertically */
    display: table-cell;
    vertical-align: middle;
    pointer-events:none;
}
.modal-content {
    /* Bootstrap sets the size of the modal in the modal-dialog class, we need to inherit it */
    width:inherit;
    height:inherit;
    /* To center horizontally */
    margin: 0 auto;
    pointer-events:all;
}


@media screen and (max-width: 749px) {
	.top {
		display: none;
	}
	.top-info {
		display: none;
	}
    .box {
		max-width: 750px;
		margin: auto;
		padding: 0!important;
		width: 100%;
	}

	.take {
		max-width: 750px;
		width: 100%;
		margin: auto;
		margin-top: 25px;
		position: relative;
	}
	.take-bg {
		max-width: 750px;
		width: 100%;
		margin: auto;
	}

	.top-join {
		position: absolute;
		left: 0;
		right: 0;
		bottom: 5px;
	}

	#id1 {
		height: 30px;
		margin-right:10px;
	}

	#id2 {
		height: 30px;
	}

	.take-btn {
		position:absolute;
		bottom:10%;
		left:40%;
		height: 30%;
	}

	.lc {
		max-width: 750px;
		width: 100%;
	}

	.zysx {
		height:30px;left:20%;top:20px;
		position: absolute;
	}
	.lxwm {
		height:30px;left:55%;margin-top:20px;
		position: absolute;
	}
	.zxbg {
		float:left;
		width: 100%;
	}
}
@media screen and (min-width: 750px) {
    .box {
		min-width: 750px;
		margin: auto;
		padding: 0!important;
	}

	.take {
		width: 750px;
		margin: auto;
		margin-top: 25px;
		position: relative;
	}
	.take-bg {
		width: 750px;
	}
	
	.top-join {
		position: absolute;
		left: 0;
		right: 0;
		bottom: 20px;
	}
	#id1 {
		height: 60px;
		margin-right:50px;
	}

	#id2 {
		height: 60px;
	}

	.take-btn {
		position:absolute;bottom:20px;margin-left:300px;
	}

	.zysx {
		position: absolute;
		height:50px;left:20%;margin-top:50px;
	}
	.lxwm {
		height:50px;left:60%;margin-top:50px;
		position: absolute;
	}
	.zxbg {
		/*position: absolute;*/
		float:left;
		width: 817px;
	}
}
